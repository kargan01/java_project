package uygulamalar;
public class tutorial {

	public static void main(String[] args) {



int[] dizi = {4, 1, 5, 3, 2, 6, 9};
	
	for(int j = 1; j < dizi.length; j++){
		
		int key = dizi[j];
	
		int i = j - 1;	
		
		while(i >= 0 && dizi[j] > key){
			
			dizi[i+1] = dizi[i];
			i = i-1;
			dizi[i+1] = key;
			
			
		}
		for (int m = 0; m < dizi.length; m++){
			System.out.println(dizi[m]);}
	
}
	
      }
	
	}
